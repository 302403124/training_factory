{% extends 'Admin/adminbase.html.twig' %}
{% block body %}
    <center>
        <table class="table" style="width: 50%;">
            <thead class="thead" style="color: white; background-color: rgba(21, 22, 22, 0.76);">
            <tr>
                {#            <th scope="col">naam</th>#}
                <th scope="col">Email</th>
                <th scope="col">Edit</th>
                <th scope="col">Delete</th>
            </tr>
            </thead>
            <tbody>
            {% for user in users %}
                <tr class ="text-white" style="background-color: rgba(21, 22, 22, 0.76);">
                    {#                    <td>{{ user.naam }}</td>#}
                    <td>{{ user.email }}</td>
                    <td>
                        <a href="{{ path ('EditUser',{'item':user.id}) }}">Edit</a>
                    </td>
                    <td>
                        <a href="{{ path ('DeleteUser',{'item':user.id}) }}">Delete</a>
                    </td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
    </center>
{% endblock %}